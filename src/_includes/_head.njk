<head>
  <title>{{ title }}</title>
  {%- include "_meta.njk" -%}

  <!-- Preload critical fonts to reduce render-blocking latency -->
  <link rel="preload" as="font" type="font/woff2" href="/assets/fonts/Ronzino-Regular.woff2" crossorigin>
  <link rel="preload" as="font" type="font/woff2" href="/assets/fonts/Ronzino-Bold.woff2" crossorigin>
  <link rel="preload" as="font" type="font/woff2" href="/assets/fonts/Ronzino-Medium.woff2" crossorigin>
  <link rel="preload" as="font" type="font/woff2" href="/assets/fonts/Ronzino-Oblique.woff2" crossorigin>
  <link rel="preconnect" href="https://res.cloudinary.com" crossorigin>
  {% if "/playground" not in page.url and "/colophon" not in page.url %}
    <link rel="preconnect" href="https://app.cal.com" crossorigin>
  {% endif %}
  <link rel="preload" as="style" href="{{ '/css/style.css' | url }}" />
  <link rel="stylesheet" href="{{ '/css/style.css' | url }}" />
  <link rel="apple-touch-icon" sizes="180x180" href="/assets/favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/assets/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/assets/favicon/favicon-16x16.png">
  <link rel="manifest" href="/assets/favicon/site.webmanifest">
</head>
