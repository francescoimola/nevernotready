<!DOCTYPE html>
<html lang="en">
  <head>
    <title>{{ title }}</title>
    {%- include "_meta.html" -%}
    <!-- Resource hints -->
    <link rel="preload" as="style" href="{{ '/css/style.css' | url }}"/>
    <link rel="preload" href="/fonts/InterVariable.woff2" as="font" type="font/woff2" crossorigin>
    <link
    rel="stylesheet" href="{{ '/css/style.css' | url }}"/>
    <!-- Critical JS -->
    <script src="/assets/js/unorphanise.js" defer></script>
    <script
      src="https://unpkg.com/cloudinary-core@latest/cloudinary-core-shrinkwrap.js"
      type="text/javascript">
    </script>
  </head>
  <body>
    {%- include "_header.njk" -%}
    <div class="wrapper">
        {{ content | safe }}
    </div>
    {%- include "_footer.njk" -%}
    <script src="/assets/js/slideframe.js" defer></script>
    <script type="text/javascript">
      var cloudinary = cloudinary.Cloudinary.new({cloud_name: "<YOUR_CLOUD_NAME>"}); 
      cloudinary.responsive();
  </script>
  </body>
</html>